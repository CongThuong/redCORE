<?xml version="1.0" ?>
<contentelement type="contentelement">
    <name>Weblinks</name>
    <author>redCOMPONENT</author>
    <copyright>Copyright (C) 2008 - 2016 redCOMPONENT.com. All rights reserved.</copyright>
    <version>1.8.3</version>
    <description>Translation definition for Joomla Weblinks component</description>
    <reference>
        <table name="weblinks">
            <field type="referenceid" name="id" translate="0">ID</field>
            <field type="titletext" name="title" translate="1">Title</field>
            <field type="textarea" name="description" translate="1">Description</field>
            <field type="text" name="url" maxlength="1000" translate="1">URL</field>
            <field type="state" name="state" translate="1" alwaysFallback="true">State</field>
        </table>
        <component>
            <editForm admin="true" option="com_weblinks" view="weblink" layout="edit" identifier="id" showbutton="true" htmlposition=".btn-toolbar:first" />
        </component>
    </reference>
</contentelement>
