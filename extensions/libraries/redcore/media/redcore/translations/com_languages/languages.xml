<?xml version="1.0" ?>
<contentelement type="contentelement">
    <name>Languages</name>
    <author>redCOMPONENT</author>
    <copyright>Copyright (C) 2008 - 2016 redCOMPONENT.com. All rights reserved.</copyright>
    <version>1.8.3</version>
    <description>Translation definition for Joomla Languages</description>
    <reference type="content">
        <table name="languages">
            <field type="referenceid" name="lang_id" translate="0">ID</field>
            <field type="titletext" name="title" translate="1">Title</field>
            <field type="state" name="published" translate="1" alwaysFallback="true">State</field>
        </table>
        <component>
            <editForm admin="true" option="com_languages" view="language" layout="edit" identifier="lang_id" showbutton="true" htmlposition=".btn-toolbar:first" />
        </component>
    </reference>
</contentelement>
